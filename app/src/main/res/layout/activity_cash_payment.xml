<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CashPayActivity">

    <LinearLayout
        android:id="@+id/linearLayout8"
        style="@style/linearlayout7_style"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <ImageView
            android:id="@+id/imageView2"
            style="@style/imageview14_style"
            android:layout_marginTop="390dp"/>


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout55"
            android:layout_marginTop="20dp"
            style="@style/constlayout7_style">

            <androidx.cardview.widget.CardView
                android:id="@+id/cardView1"
                style="@style/cardview3_style"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <EditText
                    android:id="@+id/buttonn"
                    style="@style/button18_style" />

            </androidx.cardview.widget.CardView>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout"
            style="@style/constlayout8_style">

            <androidx.cardview.widget.CardView
                android:id="@+id/cardVieww"
                style="@style/cardview4_style"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent">

                <EditText
                    android:id="@+id/button22"
                    style="@style/button19_style" />

            </androidx.cardview.widget.CardView>
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout555"
            style="@style/constlayout8_style"
            android:layout_height="146dp">

            <androidx.cardview.widget.CardView
                android:id="@+id/cardView2"
                style="@style/cardview4_style"
                android:layout_marginBottom="90dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent">

                <EditText
                    android:id="@+id/button33"
                    style="@style/button20_style" />

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cardView"
                style="@style/cardview4_style"
                android:layout_marginTop="80dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <EditText
                    android:id="@+id/button3"
                    style="@style/button_summa_style" />

            </androidx.cardview.widget.CardView>
        </androidx.constraintlayout.widget.ConstraintLayout>

        <Button
            android:id="@+id/buy"
            style="@style/button17_style"
            android:layout_width="123dp"
            android:layout_height="50dp"
            android:layout_marginTop="40dp" />

        <ImageButton
            android:id="@+id/back"
            style="@style/imagebutton2_style"
            android:backgroundTint="@color/white"
            android:layout_marginEnd="160dp"
            android:layout_marginBottom="400dp"/>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>