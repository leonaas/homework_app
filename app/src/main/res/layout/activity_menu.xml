<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MenuActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            style="@style/linearlayout_style"
            android:layout_marginTop="90dp">

            <ImageButton
                android:id="@+id/imageButton"
                style="@style/imagebutton_style" />

            <ImageView
                android:id="@+id/imageView17"
                style="@style/imageview2_style" />

            <ImageView
                android:id="@+id/imageView18"
                style="@style/imageview3_style" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout"
            style="@style/linearlayout2_style">

            <Button
                android:id="@+id/button2"
                style="@style/button5_style" />

            <Button
                android:id="@+id/button5"
                style="@style/button6_style" />

            <Button
                android:id="@+id/button6"
                style="@style/button7_style" />

            <Button
                android:id="@+id/button7"
                style="@style/button8_style" />

            <Button
                android:id="@+id/button8"
                style="@style/button9_style" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="175dp"
            android:layout_marginBottom="90dp"
            android:gravity="center"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/basket"
                style="@style/imageview11_style"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="250dp" />

            <LinearLayout
                android:id="@+id/linearlayout11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical"
                android:layout_marginBottom="10dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent">

                <ImageView
                    android:id="@+id/imageView9"
                    style="@style/imageview4_style"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    tools:layout_editor_absoluteY="584dp" />

                <ImageView
                    android:id="@+id/account"
                    style="@style/imageview5_style"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent" />
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>